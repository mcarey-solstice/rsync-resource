{
  "command": "rsync",
  "options": {
    "force": "true",
    "perms": "true",
    "progress": "true",
    "copy-unsafe-links": "true",
    "safe-links": "true",
    "compress": "true",
    "rsh": "ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null",
    "update": "true",
    "times": "true",
    "port": "22",
    "recursive": "true"
  },
  "client": {
    "hostname": "server",
    "username": "root",
    "filename": "./."
  },
  "server": {
    "hostname": "",
    "username": "",
    "filename": "."
  }
}
